{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 3,
    "name": "block-library/carousel",
    "version": "1.0.0",
    "title": "Carousel",
    "category": "widgets",
    "icon": "smiley",
    "description": "Carousel to render cards in a slider",
    "example": {},
    "supports": {
        "html": false
    },
    "attributes": {
        "_settings_num_cards": {
            "type": "number",
            "default": 3
        },
        "body": {
            "type": "string",
            "default": ""
        }
    },
    "allowedBlocks": [
        "block-library/carousel-card"
    ],
    "textdomain": "carousel",
    "editorScript": "file:./index.js",
    "editorStyle": "file:./index.css",
    "style": "file:./style-index.css",
    "render": "file:./render.php",
    "viewScript": "file:./view.js"
}